<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>MIYO_Weather</RootNamespace>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Qweather\QweatherIcon\IconFont\qweather-icons.ttf" />
    <Resource Include="Qweather\QweatherIcon\IconFont\qweather-icons.ttf" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Qweather\QweatherIcon\IconFont\icons-list.json" />
    <EmbeddedResource Include="Qweather\QweatherIcon\IconFont\icons-list.json" />
  </ItemGroup>

</Project>
